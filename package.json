{
  "name": "nx-remotecache-qiniu",
  "version": "1.0.0",
  "description": "Remote caching for @nrwl/nx using Qiniu Blob Storage",
  "keywords": [],
  "author": "toimc <admin@wayearn.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/toimc/nx-remotecache-qiniu.git"
  },
  "packageManager": "pnpm@8.11.0",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "node --watch-path=src dist/index.cjs",
    "release": "nrm use npm && npm publish",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "build": "unbuild",
    "build:dev": "unbuild --stub --watch",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "lint-staged": "^15.2.0",
    "nrm": "^1.2.6",
    "simple-git-hooks": "^2.9.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "node-fetch": "^2.7.0",
    "node-persist": "^4.0.1",
    "nx-remotecache-custom": "^17.1.1",
    "pkg-dir": "^5.0.0",
    "qiniu": "^7.11.0"
  }
}